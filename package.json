{
  "name": "ubersick",
  "productName": "Ubersick",
  "version": "1.0.0",
  "description": "Describe nwb-electron-starter here",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "build": "nwb build && babel main -d dist && npm run package",
    "build-electron-main": "babel main -d dist",
    "watch-electron-main": "babel main -d dist -w",
    "clean": "nwb clean",
    "start": "npm run build-electron-main && node dev-server.js --run-electron",
    "test": "nwb test",
    "test:watch": "nwb test --server",
    "package": "rm -rf ./release && electron-packager --overwrite --asar='true' --ignore='src|main|tests' --prune --platform=darwin --arch=x64 --out='release' ."
  },
  "dependencies": {
    "babel-core": "^6.18.2",
    "electron-irpc": "0.0.2",
    "electron-window-state": "^3.0.3",
    "flex-component": "^0.1.3",
    "halogen": "^0.2.0",
    "ip": "^1.1.4",
    "lodash": "^4.16.3",
    "my-local-ip": "^1.0.0",
    "nodobjc": "^2.1.0",
    "react": "^15.3.2",
    "react-addons-shallow-compare": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-grid-layout": "^0.13.9",
    "react-key-handler": "^0.3.0",
    "react-pinky-promise": "^1.0.1",
    "react-resizable": "^1.4.5",
    "weather-js": "^1.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "directory-named-webpack-plugin": "^1.1.0",
    "electron-packager": "^8.1.0",
    "electron-prebuilt": "^1.4.6",
    "electron-rebuild": "^1.2.1",
    "express": "^4.14.0",
    "minimist": "^1.2.0",
    "nwb": "^0.12.2",
    "nwb-sass": "^0.6.0",
    "webpack": "^1.13.0"
  },
  "author": "",
  "license": "MIT",
  "repository": ""
}
